{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "extension": [".ts", ".tsx"],
    "exclude": [
        ".aws-sam",
        ".eslintrc.js",
        "webpack.config.js",
        "coverage",
        "default_data",
        "spec/helpers",
        "scripts",
        "src/**/*.spec*"
    ],
    "reporter": ["html", "text", "cobertura"],
    "report-dir": "coverage/report"
}
